<?xml version="1.0" encoding="utf-8"?>
<r:RDF xmlns="http://www.mozilla.org/2004/em-rdf#"
       xmlns:r="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <r:Description about="urn:mozilla:install-manifest">
    <creator>ndossougbe</creator>
    <description>
Quick and dirty web content language switcher. It disables itself after switch, that's the
expected behavior! It just swaps the first two locales of 'intl.accept_languages' and quits.
    </description>
    <homepageURL>https://github.com/ndossougbe/restartless</homepageURL>
    <id>toolazy@for.that</id>
    <name>Locale Switch</name>
    <version>0.1</version>

    <bootstrap>true</bootstrap>
    <type>2</type>

    <targetApplication>
      <r:Description>
        <id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</id>
        <minVersion>4.0b4</minVersion>
        <maxVersion>4.0.*</maxVersion>
      </r:Description>
    </targetApplication>
  </r:Description>
</r:RDF>
